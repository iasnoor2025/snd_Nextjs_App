{
  "name": "snd-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_TELEMETRY_DISABLED=1 OTEL_TRACES_SAMPLER=always_off OTEL_TRACES_SAMPLER_ARG=0 next dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:analyze:open": "cross-env ANALYZE=true BUNDLE_ANALYZER_OPEN=true npm run build",
    "build:production": "cross-env NODE_ENV=production npm run build",
    "build:clean": "rm -rf .next && rm -rf node_modules/.cache && npm run build",
    "start": "next start",
    "start:production": "cross-env NODE_ENV=production next start",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "dev:clean": "rm -rf .next && rm -rf node_modules/.cache && npm run drizzle:generate && npm run dev",
    "analyze": "ANALYZE=true npm run build",
    "analyze:bundle": "cross-env ANALYZE=true next build",
    "drizzle:introspect": "drizzle-kit introspect",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:studio": "drizzle-kit studio",
    "drizzle:push": "drizzle-kit push",
    "drizzle:drop": "drizzle-kit drop",
    "seed:saudi-locations": "tsx src/lib/drizzle/seed-saudi-locations-comprehensive.ts",
    "reset:db": "node scripts/reset-database.js",
    "migrate:db": "node scripts/run-migration.js",
    "push:schema": "node scripts/push-schema.js",
    "check:db": "node scripts/check-database.js",
    "create:admin": "node scripts/create-super-admin.js",
    "setup:production": "node scripts/full-production-setup.js",
    "check:rbac": "node scripts/check-rbac-structure.js",
    "seed:rbac": "node scripts/seed-rbac-system.js",
    "fix:employee-duplicates": "tsx scripts/fix-employee-duplicates.ts",
    "update:driving-license": "node scripts/update-driving-license-from-iqama.js",
    "extract:nationality": "tsx scripts/extract-nationality-from-iqama.ts",
    "check:redis": "node scripts/check-redis-status.js",
    "check:db-content": "tsx scripts/check-database-content.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "fix:all": "npm run format && npm run type-check && npm run build",
    "prepare": "node -e \"if (process.env.CI) process.exit(0); try { require.resolve('husky'); } catch(e) { process.exit(0); }\" && npx husky || true"
  },
