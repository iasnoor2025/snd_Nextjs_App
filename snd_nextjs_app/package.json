{
  "name": "snd-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_TELEMETRY_DISABLED=1 OTEL_TRACES_SAMPLER=always_off OTEL_TRACES_SAMPLER_ARG=0 next dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:analyze:open": "cross-env ANALYZE=true BUNDLE_ANALYZER_OPEN=true npm run build",
    "build:production": "cross-env NODE_ENV=production npm run build",
    "build:optimized": "node scripts/optimize-build.js && npm run build",
    "build:clean": "rm -rf .next && rm -rf node_modules/.cache && npm run build",
    "start": "next start",
    "start:production": "cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "db:reset": "node scripts/reset-db.js",
    "db:reset:complete": "node scripts/reset-db-complete.js",
    "db:fix-schema": "node scripts/fix-schema-inconsistencies.js",
    "db:cleanup-legacy": "node scripts/remove-legacy-tables.js",
    "dev:clean": "rm -rf .next && rm -rf node_modules/.cache && npm run drizzle:generate && npm run dev",
    "cleanup": "node scripts/cleanup-dev.js",
    "dev:fresh": "npm run cleanup && npm run dev",
    "analyze": "ANALYZE=true npm run build",
    "analyze:bundle": "cross-env ANALYZE=true next build",
    "drizzle:introspect": "drizzle-kit introspect",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:studio": "drizzle-kit studio",
    "drizzle:push": "drizzle-kit push",
    "drizzle:drop": "drizzle-kit drop",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.85.0",
    "@tanstack/react-query-devtools": "^5.85.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/formidable": "^3.4.5",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.4",
    "formidable": "^3.5.4",
    "html2canvas": "^1.4.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.539.0",
    "next": "^15.4.6",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-day-picker": "^9.8.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.5.3",
    "recharts": "^3.1.2",
    "sharp": "^0.34.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.6",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "22.17.1",
    "@types/pg": "^8.15.5",
    "@types/react": "18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "string-replace-loader": "^3.2.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "5.9.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
